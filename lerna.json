{
  "version": "0.29.0",
  "npmClient": "pnpm",
  "command": {
    "version": {
      "forcePublish": true
    },
    "publish": {
      "allowBranch": ["main", "master"],
      "conventionalCommits": true,
      "gitRemote": "origin",
      "message": "chore(release): %s"
    }
  },
  "forceLocal": true,
  "sort": true,
  "stream": true,
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}
