    "http-terminator": "^3.2.0",
    "morgan": "^1.10.0",
    "openid-client": "^5.4.3",
    "passport": "^0.6.0",
    "qs": "^6.11.2",
    "uint8arrays": "^3.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.13",
