{
  "IXStatePersistence": {
    "components": {
      "schemas": {
        "DeleteExpiredStatesArgs": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            }
          }
        },
        "DeleteStateResult": {
          "type": "boolean"
        },
        "LoadStateArgs": {
          "$ref": "#/components/schemas/GetStateArgs"
        },
        "GetStateArgs": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            }
          },
          "required": ["type"],
          "additionalProperties": false
        },
        "LoadStateResult": {
          "$ref": "#/components/schemas/State"
        },
        "State": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "step": {
              "type": "string",
              "description": "value of the state. top level of eventName. examples: acceptAgreement, enterPersonalDetails"
            },
            "type": {
              "type": "string",
              "description": "Machine id"
            },
            "eventName": {
              "type": "string",
              "description": "event name like SET_TOC, SET_FIRSTNAME, ..."
            },
            "state": {
              "type": "string",
              "description": "stringified value of the state"
            },
            "createdAt": {
              "type": "string",
              "format": "date-time"
            },
            "expiresAt": {
              "type": "string",
              "format": "date-time"
            },
            "updatedAt": {
              "type": "string",
              "format": "date-time"
            },
            "completedAt": {
              "type": "string",
              "format": "date-time"
            },
            "tenantId": {
              "type": "string"
            }
          },
          "required": ["id", "step", "type", "eventName", "state", "createdAt", "updatedAt"]
        },
        "NonPersistedMachineSnapshot": {
          "$ref": "#/components/schemas/SaveStateArgs"
        },
        "SaveStateArgs": {
          "type": "object",
          "properties": {
            "step": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "eventName": {
              "type": "string"
            },
            "state": {
              "type": "string"
            },
            "expiresAt": {
              "type": "string",
              "format": "date-time"
            },
            "completedAt": {
              "type": "string",
              "format": "date-time"
            },
            "tenantId": {
              "type": "string"
            }
          },
          "required": ["step", "type", "eventName", "state"]
        }
      },
      "methods": {
        "stateDeleteExpired": {
          "description": "Deletes the state of an xstate machine in the database.",
          "arguments": {
            "$ref": "#/components/schemas/DeleteExpiredStatesArgs"
          },
          "returnType": {
            "$ref": "#/components/schemas/DeleteStateResult"
          }
        },
        "stateLoad": {
          "description": "Loads the state of an xstate machine from the database.",
          "arguments": {
            "$ref": "#/components/schemas/LoadStateArgs"
          },
          "returnType": {
            "$ref": "#/components/schemas/LoadStateResult"
          }
        },
        "statePersist": {
          "description": "Persists the state whenever an event is emitted",
          "arguments": {
            "$ref": "#/components/schemas/NonPersistedMachineSnapshot"
          },
          "returnType": {
            "$ref": "#/components/schemas/State"
          }
        }
      }
    }
  }
}
