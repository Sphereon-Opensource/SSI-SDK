{
  "IXStatePersistence": {
    "components": {
      "schemas": {
        "DeleteExpiredStatesArgs": {
          "type": "object",
          "properties": {
            "machineType": {
              "type": "string",
              "description": "Machine type/id"
            }
          },
          "required": ["machineType"],
          "additionalProperties": false
        },
        "DeleteStateResult": {
          "type": "boolean"
        },
        "LoadActiveStateArgs": {
          "$ref": "#/components/schemas/GetActiveStateArgs"
        },
        "GetActiveStateArgs": {
          "type": "object",
          "properties": {
            "machineType": {
              "type": "string",
              "description": "Machine type/id"
            },
            "tenantId": {
              "type": "string"
            }
          },
          "required": ["machineType"]
        },
        "State": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "stateName": {
              "type": "string",
              "description": "value of the state's name. top level of event type. examples: acceptAgreement, enterPersonalDetails"
            },
            "machineType": {
              "type": "string",
              "description": "Machine type/id"
            },
            "xStateEventType": {
              "type": "string",
              "description": "event types like SET_TOC, SET_FIRSTNAME, ..."
            },
            "state": {
              "description": "state of the machine in this snapshot"
            },
            "createdAt": {
              "type": "string",
              "format": "date-time"
            },
            "expiresAt": {
              "type": "string",
              "format": "date-time"
            },
            "updatedAt": {
              "type": "string",
              "format": "date-time"
            },
            "completedAt": {
              "type": "string",
              "format": "date-time"
            },
            "tenantId": {
              "type": "string"
            }
          },
          "required": ["id", "stateName", "machineType", "xStateEventType", "state", "createdAt", "updatedAt"]
        },
        "NonPersistedMachineSnapshot": {
          "$ref": "#/components/schemas/SaveStateArgs"
        },
        "SaveStateArgs": {
          "type": "object",
          "properties": {
            "stateName": {
              "type": "string",
              "description": "value of the state's name. top level of event type. examples: acceptAgreement, enterPersonalDetails"
            },
            "machineType": {
              "type": "string",
              "description": "Machine type/id"
            },
            "xStateEventType": {
              "type": "string",
              "description": "event types like SET_TOC, SET_FIRSTNAME, ..."
            },
            "state": {
              "description": "state of the machine in this snapshot"
            },
            "expiresAt": {
              "type": "string",
              "format": "date-time"
            },
            "completedAt": {
              "type": "string",
              "format": "date-time"
            },
            "tenantId": {
              "type": "string"
            }
          },
          "required": ["stateName", "machineType", "xStateEventType", "state"]
        }
      },
      "methods": {
        "stateDeleteExpired": {
          "description": "Deletes the state of an xstate machine in the database.",
          "arguments": {
            "$ref": "#/components/schemas/DeleteExpiredStatesArgs"
          },
          "returnType": {
            "$ref": "#/components/schemas/DeleteStateResult"
          }
        },
        "stateLoadActive": {
          "description": "Loads the state of an xstate machine from the database.",
          "arguments": {
            "$ref": "#/components/schemas/LoadActiveStateArgs"
          },
          "returnType": {
            "$ref": "#/components/schemas/State"
          }
        },
        "statePersist": {
          "description": "Persists the state whenever an event is emitted",
          "arguments": {
            "$ref": "#/components/schemas/NonPersistedMachineSnapshot"
          },
          "returnType": {
            "$ref": "#/components/schemas/State"
          }
        }
      }
    }
  }
}
