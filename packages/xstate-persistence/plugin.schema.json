{
  "IXStatePersistence": {
    "components": {
      "schemas": {
        "DeleteExpiredStatesArgs": {
          "type": "object",
          "properties": {
            "duration": {
              "type": "number"
            },
            "dialect": {
              "$ref": "#/components/schemas/SQLDialect"
            }
          },
          "required": [
            "duration",
            "dialect"
          ]
        },
        "SQLDialect": {
          "type": "string",
          "enum": [
            "SQLite3",
            "PostgreSQL"
          ]
        },
        "DeleteStateResult": {
          "$ref": "#/components/schemas/VoidResult"
        },
        "VoidResult": {
          "type": "null"
        },
        "LoadStateArgs": {
          "$ref": "#/components/schemas/GetStateArgs"
        },
        "GetStateArgs": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            }
          },
          "required": [
            "type"
          ],
          "additionalProperties": false
        },
        "LoadStateResult": {
          "$ref": "#/components/schemas/State"
        },
        "State": {
          "type": "object",
          "properties": {
            "state": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "createdAt": {
              "type": "string",
              "format": "date-time"
            },
            "updatedAt": {
              "type": "string",
              "format": "date-time"
            },
            "completedAt": {
              "type": "string",
              "format": "date-time"
            },
            "tenantId": {
              "type": "string"
            }
          },
          "required": [
            "state",
            "type",
            "createdAt",
            "updatedAt"
          ]
        },
        "XStatePersistenceEvent": {
          "type": "object",
          "properties": {
            "type": {
              "$ref": "#/components/schemas/XStatePersistenceEventType"
            },
            "data": {
              "$ref": "#/components/schemas/NonPersistedXStatePersistenceEvent"
            }
          },
          "required": [
            "type",
            "data"
          ]
        },
        "XStatePersistenceEventType": {
          "type": "string",
          "const": "every"
        },
        "NonPersistedXStatePersistenceEvent": {
          "$ref": "#/components/schemas/SaveStateArgs"
        },
        "SaveStateArgs": {
          "type": "object",
          "properties": {
            "state": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "createdAt": {
              "type": "string",
              "format": "date-time"
            },
            "updatedAt": {
              "type": "string",
              "format": "date-time"
            },
            "completedAt": {
              "type": "string",
              "format": "date-time"
            },
            "tenantId": {
              "type": "string"
            }
          },
          "required": [
            "state",
            "type"
          ]
        },
        "OnEventResult": {
          "$ref": "#/components/schemas/VoidResult"
        }
      },
      "methods": {
        "deleteExpiredStates": {
          "description": "Deletes the state of an xstate machine in the database.",
          "arguments": {
            "$ref": "#/components/schemas/DeleteExpiredStatesArgs"
          },
          "returnType": {
            "$ref": "#/components/schemas/DeleteStateResult"
          }
        },
        "loadState": {
          "description": "Loads the state of an xstate machine from the database.",
          "arguments": {
            "$ref": "#/components/schemas/LoadStateArgs"
          },
          "returnType": {
            "$ref": "#/components/schemas/LoadStateResult"
          }
        },
        "onEvent": {
          "description": "Persists the state whenever an event is emitted",
          "arguments": {
            "$ref": "#/components/schemas/XStatePersistenceEvent"
          },
          "returnType": {
            "$ref": "#/components/schemas/OnEventResult"
          }
        }
      }
    }
  }
}