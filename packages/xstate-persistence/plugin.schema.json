{
  "IXStatePersistence": {
    "components": {
      "schemas": {
        "DeleteStateArgs": {
          "type": "object",
          "properties": {
            "where": {
              "type": "string"
            },
            "parameters": {
              "$ref": "#/components/schemas/ObjectLiteral"
            }
          },
          "required": [
            "where"
          ]
        },
        "ObjectLiteral": {
          "type": "object",
          "description": "Interface of the simple literal object with any string keys."
        },
        "DeleteStateResult": {
          "$ref": "#/components/schemas/VoidResult"
        },
        "VoidResult": {
          "type": "null"
        },
        "LoadStateArgs": {
          "$ref": "#/components/schemas/GetStateArgs"
        },
        "GetStateArgs": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            }
          },
          "required": [
            "type"
          ],
          "additionalProperties": false
        },
        "LoadStateResult": {
          "$ref": "#/components/schemas/State"
        },
        "State": {
          "type": "object",
          "properties": {
            "state": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "createdAt": {
              "type": "string",
              "format": "date-time"
            },
            "updatedAt": {
              "type": "string",
              "format": "date-time"
            },
            "completedAt": {
              "type": "string",
              "format": "date-time"
            },
            "tenantId": {
              "type": "string"
            },
            "ttl": {
              "type": "number"
            }
          },
          "required": [
            "state",
            "type",
            "createdAt",
            "updatedAt",
            "ttl"
          ]
        },
        "XStatePersistenceEvent": {
          "type": "object",
          "properties": {
            "type": {
              "$ref": "#/components/schemas/XStatePersistenceEventType"
            },
            "data": {
              "$ref": "#/components/schemas/NonPersistedXStatePersistenceEvent"
            }
          },
          "required": [
            "type",
            "data"
          ]
        },
        "XStatePersistenceEventType": {
          "type": "string",
          "const": "every"
        },
        "NonPersistedXStatePersistenceEvent": {
          "$ref": "#/components/schemas/SaveStateArgs"
        },
        "SaveStateArgs": {
          "type": "object",
          "properties": {
            "state": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "createdAt": {
              "type": "string",
              "format": "date-time"
            },
            "updatedAt": {
              "type": "string",
              "format": "date-time"
            },
            "completedAt": {
              "type": "string",
              "format": "date-time"
            },
            "tenantId": {
              "type": "string"
            },
            "ttl": {
              "type": "number"
            }
          },
          "required": [
            "state",
            "type",
            "ttl"
          ]
        },
        "OnEventResult": {
          "$ref": "#/components/schemas/VoidResult"
        },
        "PersistStateArgs": {
          "$ref": "#/components/schemas/SaveStateArgs"
        },
        "PersistStateResult": {
          "$ref": "#/components/schemas/VoidResult"
        }
      },
      "methods": {
        "deleteState": {
          "description": "Deletes the state of an xstate machine in the database.",
          "arguments": {
            "$ref": "#/components/schemas/DeleteStateArgs"
          },
          "returnType": {
            "$ref": "#/components/schemas/DeleteStateResult"
          }
        },
        "loadState": {
          "description": "Loads the state of an xstate machine from the database.",
          "arguments": {
            "$ref": "#/components/schemas/LoadStateArgs"
          },
          "returnType": {
            "$ref": "#/components/schemas/LoadStateResult"
          }
        },
        "onEvent": {
          "description": "Persists the state whenever an event is emitted",
          "arguments": {
            "$ref": "#/components/schemas/XStatePersistenceEvent"
          },
          "returnType": {
            "$ref": "#/components/schemas/OnEventResult"
          }
        },
        "persistState": {
          "description": "Persist the state and history of xstate machines",
          "arguments": {
            "$ref": "#/components/schemas/PersistStateArgs"
          },
          "returnType": {
            "$ref": "#/components/schemas/PersistStateResult"
          }
        }
      }
    }
  }
}